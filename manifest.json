{
  "name": "Phone Territory Manager",
  "description": "Manage phone territories.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "images/icon-16.png", "128": "images/icon-128.png" },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage",
    "*://*/*",
    "webview"
  ]
}